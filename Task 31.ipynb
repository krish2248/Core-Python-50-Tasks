   "source": [
    "# Task 31: Factorial Calculator\n",
    "# Problem: Calculate factorial of a number using while loop.\n",
    "\n",
    "n = int(input(\"Enter a number: \"))\n",
    "result = 1\n",
    "counter = 1\n",
    "\n",
    "while counter <= n:\n",
    "    result *= counter\n",
    "    counter += 1\n",
    "\n",
    "print(f\"Factorial of {n} is {result}\")"
   ]
  }
