   "source": [
    "\n",
    "# Task 38: Reverse Number\n",
    "# Problem: Reverse the digits of a number using while loop.\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "reversed_num = 0\n",
    "\n",
    "while num > 0:\n",
    "    digit = num % 10\n",
    "    reversed_num = reversed_num * 10 + digit\n",
    "    num //= 10\n",
    "\n",
    "print(f\"Reversed number: {reversed_num}\")"
   ]
  }
